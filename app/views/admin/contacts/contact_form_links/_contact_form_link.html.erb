<tr>
  <td><%= contact_form_link.title %></td>
  <td><%= contact_form_link.description %></td>
  <td><%= link_to contact_form_link.link, contact_form_link.link %></td>
  <td>
    <%= link_to 'Edit', edit_admin_contact_contact_form_link_path(contact, contact_form_link), class: 'btn btn-small' %>
    <%= link_to 'Remove', admin_contact_contact_form_link_path(contact, contact_form_link), method: :delete, data: { confirm: 'Are you sure?' }, data: { disable: 'Working' }, class: 'btn btn-small btn-danger' %>
  </td>
</tr>
