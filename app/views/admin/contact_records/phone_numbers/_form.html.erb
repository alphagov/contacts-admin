<%= simple_form_for [:admin, contact_record, phone_number] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :number %>
  <%= f.input :open_hours %>

  <div class='form-actions'>
    <%= f.submit id: 'phone-number-submit', class: 'btn btn-primary', data: { disable_with: 'Working' } %>
  </div>
<% end %>
